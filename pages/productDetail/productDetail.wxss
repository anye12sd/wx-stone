/* pages/productDetail/productDetail.wxss */
.t{
  display: block;
  font-size: 16px;
  color: #666;
  margin-left: 20px;
}
.img-box{
  text-align: center
}
.img-img{
  margin-top: 6px;
}
.effect{
  text-align: center;
  margin-top: 10px;
}

.lineBox{
  font-size: 16px;
  padding:15px 0;  
  color:#666;
  position: relative;
}
.line::before{
  content:"";
  position:absolute;
  top:52%;
  background: #999;
  left:-25px;
  width: 25%;
  height: 1px;
}
.line::after{
  content:"";
  position:absolute;
  top:52%;
  background: #999;
  right:-25px;
  width: 25%;
  height: 1px;
}
.lineBox-bottom{
  width:100%;
  height:1px;
  background: #eee
}
.tel-icon{
  width:48px;
  height:48px;
  position: fixed;
  right:0;
  bottom:120px;
  z-index: 100;
  border-radius: 50%;
}